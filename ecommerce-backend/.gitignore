# Node.js backend .gitignore
# See https://github.com/github/gitignore/blob/main/Node.gitignore

# dependencies
node_modules/

# testing
coverage/
.nyc_output/

# production
dist/
build/

# logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# runtime data
pids/
*.pid
*.seed
*.pid.lock

# environment
.env
.env.*

# Also ignore local env variants present in root
.env*.local

# OS files
.DS_Store

# Editors
.vscode/
.idea/
